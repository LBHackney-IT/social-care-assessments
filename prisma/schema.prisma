datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Submission {
  id           String    @id @default(cuid())
  socialCareId Int
  formId       String
  data         Json?
  editedBy     String[]
  createdBy    String
  createdAt    DateTime  @default(now())
  updatedAt    DateTime  @default(now())
  submittedAt  DateTime?
}
