datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Submission {
  id           Int      @id @default(autoincrement())
  socialCareId Int
  formId       String
  data         Json
  createdBy    String
  submittedAt  DateTime
  createdAt    DateTime @default(now())
  updatedAt    DateTime @default(now())
}
